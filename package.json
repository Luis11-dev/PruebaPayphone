{
  "name": "newman-runner",
  "version": "1.0.0",
  "description": "Proyecto para correr colecciones de Postman con Newman",
  "scripts": {
    "test": "newman run collections/PruebaPayphone.postman_collection.json -e environments/Test.postman_environment.json",
    "test-report": "newman run collections/PruebaPayphone.postman_collection.json -e environments/Test.postman_environment.json -r htmlextra --reporter-htmlextra-export ./results"
  },
  "devDependencies": {
    "newman": "^6.0.0"
  },
  "dependencies": {
    "newman-reporter-htmlextra": "^1.23.1"
  }
}
